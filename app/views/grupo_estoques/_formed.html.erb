             <h1 class="modal-title" id="myModalLabel">Atualizar grupo de estoques</h1>
            </div>
            </div>
            </div>
           
            <div class="modal-body">
          
            <!--formulario clientes -->



<%= form_for(@grupo_estoque) do |f| %>
  <% if @grupo_estoque.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@grupo_estoque.errors.count, "error") %> prohibited this grupo_estoque from being saved:</h2>

      <ul>
      <% @grupo_estoque.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field form-horizontal">
    <%= f.label :nome, class: "sr-only" %><br>
    <div class="input-group col-md-12">
      <%= f.text_field :nome, class: "form-control col-sm-12", id: "exampleInputAmount", placeholder: "Nome"%> 
     </div>
  </div>

  <div class="field form-horizontal">
    <%= f.label :descricao_loga, class: "sr-only" %>
