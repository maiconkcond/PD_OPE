<tr id="client_<%= client.id %>">
  <td><%= client.nome %></td>
  <td><%= client.endereco %></td>
  <td><%= client.complemento %></td>
  <td><%= client.bairro %></td>
  <td><%= client.cep %></td>
  <td><%= client.tel1 %></td>
  <td><%= client.tel2 %></td>
  <td><%= client.cel1 %></td>
  <td><%= client.cel2 %></td>
  <td><%= client.email %></td>
  <td class="botoes-acao">
    <%= link_to image_tag('editar.png'), edit_client_path(client), remote: true, :class => 'btn btn-sm btn-info botoes-acao' %>
    <%= link_to image_tag('excluir.png'), client, remote: true, :method => :delete, data: {confirm: "Excluir cliente #{client.nome} ?"}, :class => 'btn btn-sm btn-danger botoes-acao' %>
  </td>
</tr>